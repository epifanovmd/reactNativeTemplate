{
  "name": "reactNativeTemplate",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "start": "react-native start",
    "test": "jest",
    "lint": "eslint \"src/**/*{.ts,.tsx}\"",
    "lint:fix": "eslint --fix \"src/**/*{.ts,.tsx}\"",
    "prettier:fix": "prettier --parser typescript --write src/**/*.{json,ts,tsx}",
    "check-packages-updates": "yarn outdated"
  },
  "dependencies": {
    "@react-navigation/bottom-tabs": "^5.11.1",
    "@react-navigation/native": "^5.8.9",
    "@reduxjs/toolkit": "^1.4.0",
    "@types/react-i18next": "^8.1.0",
    "@types/styled-components": "^5.1.4",
    "i18next": "^19.8.3",
    "query-string": "^6.13.7",
    "react": "16.13.1",
    "react-i18next": "^11.7.3",
    "react-native": "0.63.3",
    "react-native-localize": "^2.0.0",
    "react-native-safe-area-context": "^3.1.9",
    "react-native-screens": "^2.15.0",
    "react-redux": "^7.2.2",
    "redux-thunk": "^2.3.0",
    "socket.io-client": "^3.0.2",
    "styled-components": "^5.2.1",
    "yup": "^0.29.3"
  },
  "devDependencies": {
    "@babel/core": "^7.8.4",
    "@babel/runtime": "^7.8.4",
    "@react-native-community/eslint-config": "^1.1.0",
    "@types/query-string": "^6.3.0",
    "@types/react-native": "^0.63.2",
    "@types/react-redux": "^7.1.11",
    "@types/redux-thunk": "^2.1.0",
    "@types/socket.io-client": "^1.4.34",
    "@types/yup": "^0.29.9",
    "@typescript-eslint/eslint-plugin": "^2.27.0",
    "@typescript-eslint/parser": "^2.27.0",
    "babel-jest": "^25.1.0",
    "eslint": "^6.5.1",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-react": "^7.21.5",
    "eslint-plugin-react-hooks": "^4.2.0",
    "husky": "^4.3.0",
    "lint-staged": "^10.5.1",
    "metro-react-native-babel-preset": "^0.59.0",
    "prettier": "^2.1.2",
    "typescript": "^3.8.3"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "eslint --fix \"src/**/*{.ts,.tsx}\"",
      "prettier --parser typescript --write src/**/*.{ts,tsx}",
      "git add"
    ]
  },
  "jest": {
    "preset": "react-native",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ]
  }
}
